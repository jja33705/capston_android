<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="300dp"
    android:layout_height="400dp"
    android:layout_gravity="center"
    tools:context=".activity.RankMatchingActivity">

    <TextView
        android:id="@+id/tv_vs_user_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="dddddd"
        android:textSize="50sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.503"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.132"></TextView>

    <TextView
        android:id="@+id/tv_vs_post_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="알랄라라라랄라라"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_vs_user_name"
        app:layout_constraintVertical_bias="0.181"></TextView>

    <TextView
        android:id="@+id/tv_vs_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ddd:ddd:dd:d"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_vs_post_name"
        app:layout_constraintVertical_bias="0.152"></TextView>

    <TextView
        android:id="@+id/tv_vs_speed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ddddkm/h"
        app:layout_constraintBottom_toTopOf="@+id/button_start"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_vs_time"
        app:layout_constraintVertical_bias="0.609"></TextView>

    <Button
        android:id="@+id/button_start"
        android:layout_width="200dp"
        android:layout_height="60dp"
        android:layout_marginBottom="68dp"
        android:text="시작"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"></Button>
</androidx.constraintlayout.widget.ConstraintLayout>